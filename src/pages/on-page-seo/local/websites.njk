<div class="container mb-100">

    <div class="mtb-50 back-white paa-50">
        <h1>Websites</h1>
        <p>Learn More About Websites</p>
    </div>

    <div class="mtb-50 flex flex-wrap mtb-50">
        {% for post in collections.websites | reverse %}
            {% if loop.index0 < 10 %}
                <div class="inner-container back-white paa-25">
                    <a href="{{post.url}}"><h3 class="h4">{{post.data.title}}</h3></a>
                </div>
            {% endif %}
        {% endfor %}
    </div>
</div>